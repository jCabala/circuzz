{
  "experiment": {
    "enable_exit_on_failure": false,
    "enable_working_dir_cleanup": true,

    "prove_and_verify_tuning_strategy": "time",
    "prove_and_verify_tuning_percentage": 0.5
  },

  "mina": {
    "boundary_input_probability": 0.1,
    "test_iterations": 2
  },

  "ir": {
    "operators": {
      "relations": ["==", "!="],
      "boolean_unary_operators": ["!"],
      "boolean_binary_operators": ["&&", "||"],
      "arithmetic_unary_operators": [],
      "arithmetic_binary_operators": [],

      "is_arithmetic_ternary_supported": false,
      "is_boolean_ternary_supported": true
    },

    "generation": {
      "generator": "boolean",

      "constant_probability_weight": 1,
      "variable_probability_weight": 1,
      "unary_probability_weight": 1,
      "binary_probability_weight": 1,
      "relation_probability_weight": 0,
      "ternary_probability_weight": 1,

      "max_expression_depth": 4,
      "min_number_of_assertions": 0,
      "max_number_of_assertions": 2,
      "min_number_of_input_variables": 1,
      "max_number_of_input_variables": 2,
      "min_number_of_output_variables": 0,
      "max_number_of_output_variables": 2,

      "max_exponent_value": 1,
      "boundary_value_probability": 0.25
    },

    "rewrite": {
      "weakening_probability": 0,
      "min_rewrites": 1,
      "max_rewrites": 64,
      "rules": {
        "equivalence": [
          {
            "name": "comm-and",
            "match": "(?a && ?b)",
            "rewrite": "(?b && ?a)"
          },
          { "name": "comm-or", "match": "(?a || ?b)", "rewrite": "(?b || ?a)" },
          {
            "name": "assoc-and",
            "match": "((?a && ?b) && ?c)",
            "rewrite": "(?a && (?b && ?c))"
          },
          {
            "name": "assoc-and-rev",
            "match": "(?a && (?b && ?c))",
            "rewrite": "((?a && ?b) && ?c)"
          },
          {
            "name": "assoc-or",
            "match": "((?a || ?b) || ?c)",
            "rewrite": "(?a || (?b || ?c))"
          },
          {
            "name": "assoc-or-rev",
            "match": "(?a || (?b || ?c))",
            "rewrite": "((?a || ?b) || ?c)"
          },
          { "name": "double-neg", "match": "(!(!?a))", "rewrite": "?a" },
          {
            "name": "double-neg-rev",
            "match": "?a:bool",
            "rewrite": "(!(!?a))"
          },
          {
            "name": "demorgan-and",
            "match": "(!(?a && ?b))",
            "rewrite": "((!?a) || (!?b))"
          },
          {
            "name": "demorgan-and-rev",
            "match": "((!?a) || (!?b))",
            "rewrite": "(!(?a && ?b))"
          },
          {
            "name": "demorgan-or",
            "match": "(!(?a || ?b))",
            "rewrite": "((!?a) && (!?b))"
          },
          {
            "name": "demorgan-or-rev",
            "match": "((!?a) && (!?b))",
            "rewrite": "(!(?a || ?b))"
          },
          { "name": "idem-and", "match": "(?a && ?a)", "rewrite": "?a" },
          {
            "name": "idem-and-rev",
            "match": "?a:bool",
            "rewrite": "(?a && ?a)"
          },
          { "name": "idem-or", "match": "(?a || ?a)", "rewrite": "?a" },
          { "name": "idem-or-rev", "match": "?a:bool", "rewrite": "(?a || ?a)" }
        ],
        "weakening": []
      }
    }
  }
}
